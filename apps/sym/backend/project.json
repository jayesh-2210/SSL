{
    "name": "sym-backend",
    "$schema": "../../../node_modules/nx/schemas/project-schema.json",
    "sourceRoot": "apps/sym/backend/src",
    "projectType": "application",
    "targets": {
        "serve": {
            "executor": "nx:run-commands",
            "options": {
                "command": "node --watch src/index.js",
                "cwd": "apps/sym/backend"
            }
        },
        "build": {
            "executor": "nx:run-commands",
            "options": {
                "command": "echo 'Backend uses source directly â€” no build step needed'",
                "cwd": "apps/sym/backend"
            }
        }
    }
}